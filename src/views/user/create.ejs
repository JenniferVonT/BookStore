<div id="createUserPage">
  <h1>Sign up below!</h1>
  <form id="createForm" method="POST" autocomplete="off" action="./user/create">
    <div id="createInputs">
      <input autocomplete="off" type="text" class="formControl" id="fnameInput" name="fname" placeholder="First name*" required>
      <input autocomplete="off" type="text" class="formControl" id="lnameInput" name="lname" placeholder="Last name*" required>
      <input autocomplete="off" type="text" class="formControl" id="addressInput" name="address" placeholder="Address*" required>
      <input autocomplete="off" type="text" class="formControl" id="cityInput" name="city" placeholder="City*" required>
      <input autocomplete="off" type="text" class="formControl" id="zipInput" name="zip" placeholder="Zipcode*">
      <input autocomplete="off" type="text" class="formControl" id="phoneInput" name="phone" placeholder="Phonenumber">
      <input autocomplete="off" type="text" class="formControl" id="emailInput" name="email" placeholder="Email*" required>
      <input autocomplete="off" type="text" class="formControl" id="passwordInput" name="password" placeholder="Password*" required>
      <input autocomplete="off" type="text" class="formControl" id="passwordInputCheck" name="passwordCheck" placeholder="Repeat password*" required>
    </div>
    <i>* = required</i>
    <button type="submit" class="btn">Login</button>
  </form>
</div>